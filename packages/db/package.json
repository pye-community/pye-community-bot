{
  "name": "@pye-community-bot/db",
  "version": "1.0.0",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./lib/*.d.ts",
      "node": {
        "types": "./lib/*.d.ts",
        "default": "./lib/*.js"
      },
      "default": "./lib/*.js"
    },
    ".": {
      "types": "./lib/index.d.ts",
      "node": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "default": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "lib/**/*",
    "lib/**/*.{js,map}",
    "src/**/*",
    "src/**/*.{ts,json}"
  ],
  "engines": {
    "node": ">=19.0.0"
  },
  "scripts": {
    "build": "packemon build --addEngines --addExports --declaration",
    "clean": "rm -rf .turbo node_modules",
    "push": "prisma db push --skip-generate",
    "generate": "prisma generate",
    "dev": "packemon watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "packemon": {
    "format": "lib",
    "platform": "node",
    "support": "experimental"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "@pye-community-bot/env": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^8.51.0",
    "prisma": "^5.4.2",
    "typescript": "^5.2.2"
  }
}
