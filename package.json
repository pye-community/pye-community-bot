{
  "name": "pye-community",
  "version": "1.0.0",
  "description": "A community bot created for https://discord.gg/programacion (PyE server)",
  "author": {
    "name": "PyE Community"
  },
  "engines": {
    "node": ">=19.0.0",
    "pnpm": ">=8.5.0"
  },
  "scripts": {
    "postinstall": "pnpm db:generate && pnpm build && node dist/deploy-commands.js",
    "build": "tsc && tsc-alias",
    "db:push": "prisma db push",
    "db:format": "prisma format",
    "db:generate": "prisma generate",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@huggingface/inference": "^2.6.4",
    "@prisma/client": "^5.4.2",
    "@t3-oss/env-core": "^0.7.1",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "eslint": "^8.51.0",
    "prisma": "^5.4.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
